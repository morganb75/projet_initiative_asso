{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:4173"
      ],
      "startServerCommand": "http-server ./build -p 4173",
      "startServerReadyPattern": "Available on",
      "numberOfRuns": 1
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "preset": "lighthouse:recommended"
    }
  }
}
